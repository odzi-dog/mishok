{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/protos/src",
  "projectType": "application",
  "targets": {
    "rm-output": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx rimraf ./generated",
        "cwd": "packages"
      }
    },
    "gen": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node ../../tools/buf/execute generate",
        "cwd": "packages/protos"
      },
      "dependsOn": ["rm-output"]
    }
  },
  "tags": []
}
